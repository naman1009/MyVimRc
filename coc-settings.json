{
    "java.configuration.runtimes": [
        {
            "name": "JavaSE-11",
            "path": "~/.local/bin/java",
            "default": true
        }
    ],
    "pairs.enableCharacters": ["(", "[", "{", "'", "\"", "`"],
    "coc.preferences.formatOnSaveFiletypes": [
        "css",
        "jsx",
        "tsx",
        "js",
        "ts",
        "html",
        "python",
        "json",
        "java"
    ],
    "prettier.tabWidth": 4,
    "languageserver": {
        "godot": {
            "host": "127.0.0.1",
            "filetypes": ["gdscript"],
            "port": 6008
        }
    },
    "snippets.ultisnips.directories": [
        "UltiSnips",
        "~/.vim/plugged/vim-snippets/UltiSnips"
    ]
}
